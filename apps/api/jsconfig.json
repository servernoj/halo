{
    "compilerOptions": {
        "checkJs": true,
        "strict": true,
        "noImplicitAny": false,
        "useUnknownInCatchVariables": false,
        "strictNullChecks": false,
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "target": "esnext",
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "resolveJsonModule": true,
        "skipLibCheck": true
    }
}